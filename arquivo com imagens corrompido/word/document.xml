><w:p w14:paraId="63999ED8" w14:textId="1BE84825" w:rsidR="009A682B" w:rsidRDefault="009A682B"><w:r><w:t xml:space="preserve">Nacionalidade: brasileira</w:t></w:r><w:proofErr w:type="spellStart"/><w:proofErr w:type="gramStart"/><w:r w:rsidRPr="009A682B"><w:t></w:t></w:r><w:proofErr w:type="gramEnd"/><w:r w:rsidRPr="009A682B"><w:t></w:t></w:r><w:proofErr w:type="spellEnd"/><w:r><w:t></w:t></w:r></w:p><w:p w14:paraId="455EED9A" w14:textId="23C57EC0" w:rsidR="009A682B" w:rsidRDefault="009A682B"><w:r><w:t xml:space="preserve">CPF: 027.073.725-10</w:t></w:r><w:proofErr w:type="spellStart"/><w:proofErr w:type="gramStart"/><w:r w:rsidRPr="009A682B"><w:t></w:t></w:r><w:proofErr w:type="gramEnd"/><w:r w:rsidRPr="009A682B"><w:t></w:t></w:r><w:proofErr w:type="spellEnd"/><w:r><w:t></w:t></w:r></w:p><w:p w14:paraId="0B6E14FD" w14:textId="52F6D3AC" w:rsidR="009A682B" w:rsidRDefault="009A682B"><w:r><w:t xml:space="preserve">Escolaridade: Ensino Fundamental Completo</w:t></w:r><w:proofErr w:type="spellStart"/><w:proofErr w:type="gramStart"/><w:r w:rsidRPr="009A682B"><w:t></w:t></w:r><w:proofErr w:type="gramEnd"/><w:r w:rsidRPr="009A682B"><w:t></w:t></w:r><w:proofErr w:type="spellEnd"/><w:r><w:t></w:t></w:r></w:p><w:p w14:paraId="7DA51622" w14:textId="77777777" w:rsidR="009A682B" w:rsidRDefault="009A682B"/><w:p w14:paraId="1088E9B3" w14:textId="18F3BBDB" w:rsidR="009A682B" w:rsidRPr="00A50F2E" w:rsidRDefault="009A682B"><w:pPr><w:rPr><w:b/><w:bCs/></w:rPr></w:pPr><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>História laboral:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
        </w:rPr>
        <w:t xml:space="preserve">Refere a </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
          <w:b/>
        </w:rPr>
        <w:t xml:space="preserve">Reclamante </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
        </w:rPr>
        <w:t xml:space="preserve">que ingressou na </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
          <w:b/>
        </w:rPr>
        <w:t xml:space="preserve">Reclamada </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
        </w:rPr>
        <w:t xml:space="preserve">localizada na cidade de Salvador em 2023</w:t>
      </w:r>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">  </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
        </w:rPr>
        <w:t xml:space="preserve">na função de desenvolvendo as atividades laborativas de auxiliar. Informa jornada de trabalho de 8 horas diariamente com uma hora de intervalo para o almoço. Recebia </w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
          <w:u w:val="single"/>
        </w:rPr>
        <w:t xml:space="preserve">uniforme</w:t>
      </w:r>
      <w:r>
        <w:rPr>
          <w:sz w:val="24"/>
        </w:rPr>
        <w:t xml:space="preserve">, bota e todos os EPI’s.</w:t>
      </w:r>
    </w:p><w:p w14:paraId="151B71F8" w14:textId="77777777" w:rsidR="00C834FF" w:rsidRDefault="00C834FF" w:rsidP="00C834FF"><w:pPr><w:jc w:val="both"/></w:pPr></w:p><w:p w14:paraId="664F5124" w14:textId="6379B2F7" w:rsidR="00C834FF" w:rsidRPr="00A50F2E" w:rsidRDefault="00C834FF" w:rsidP="00C834FF"><w:pPr><w:jc w:val="both"/><w:rPr><w:b/><w:bCs/></w:rPr></w:pPr><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>Benefícios previdenciários:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Houve afastamento perante INSS em 15/03/2020 como benefício auxílio doença , espécie afastamento temporário. Consta emissão de CAT (Comunicação de Acidente de Trabalho) pela empresa.</w:t>
      </w:r>
    </w:p><w:p w14:paraId="4AC8549D" w14:textId="77777777" w:rsidR="00A50F2E" w:rsidRDefault="00A50F2E" w:rsidP="00C834FF"><w:pPr><w:jc w:val="both"/></w:pPr></w:p><w:p w14:paraId="47CD4488" w14:textId="34112021" w:rsidR="00A50F2E" w:rsidRPr="00A50F2E" w:rsidRDefault="00A50F2E" w:rsidP="00C834FF"><w:pPr><w:jc w:val="both"/><w:rPr><w:b/><w:bCs/></w:rPr></w:pPr><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>Antecedentes:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Nega doenças endócrinas, reumáticas ou de caráter hereditário na família. Refere cirurgia . Nega tabagismo e etilismo. É sedentário.</w:t>
      </w:r>
    </w:p><w:p w14:paraId="7A89D92D" w14:textId="77777777" w:rsidR="00A50F2E" w:rsidRDefault="00A50F2E" w:rsidP="00C834FF"><w:pPr><w:jc w:val="both"/></w:pPr></w:p><w:p w14:paraId="414E816B" w14:textId="03AC48B4" w:rsidR="00A50F2E" w:rsidRDefault="00A50F2E" w:rsidP="00C834FF"><w:pPr><w:jc w:val="both"/></w:pPr><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>História da doença atual:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Refere o Reclamante que a partir de 2021 apresentou transtorno de ansiedade.</w:t>
      </w:r>
    </w:p><w:p w14:paraId="1D780817" w14:textId="209E0A8E" w:rsidR="00B018C5" w:rsidRDefault="00B018C5" w:rsidP="00B018C5"><w:pPr><w:jc w:val="both"/></w:pPr><w:r><w:rPr><w:b/><w:bCs/></w:rPr><w:lastRenderedPageBreak/><w:t>Exame físico</w:t></w:r><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Paciente de 38 anos de idade, destro, pesando 100 quilos, medindo 1,82m de altura, IMC 29 , com bom estado geral e nutricional, hidratado, corado, anictérico, apirético, lúcido, bem orientado no tempo e no espaço sem sinais de humor deprimido. Aparenta idade física compatível com a idade cronológica. Deambula normalmente.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve"> </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Exame específico dos membros superiores e coluna vertebral: a inspeção não apresenta nódulos, saliências ósseas, vermelhidão, deformidades ou assimetria.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve"> </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Região cervical: ausência de dor à palpação dos corpos vertebrais. Apresenta amplitude de movimentos (flexão, extensão, rotação e lateralização) preservados. Ausência de contratura para vertebral. Spurling negativo. </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve"> </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Região lombar: ausência de dor à palpação dos corpos vertebrais. Apresenta amplitude de movimentos preservados. Ausência de contratura para vertebral. Movimento de flexão do tronco preservado. Deambula com ante pés e retro pés. Sinal de Laségue negativo.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve"> </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Região ombro-braço: ausência de dor a palpação dos ombros. Movimento de rotação externa, interna e elevação dos braços preservada. Testes irritativos para ombros negativo (Neer, Jobe, Appley). Ausência de crepitação.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve"> </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Região cotovelo: Ausência de dor à palpação dos epicôndilos. Apresenta amplitude de movimentos preservados. Teste de Cozen e Mill negativos.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve"> </w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Região punho-mão: Movimento de flexão do punho preservado. Sinal de Phalen e Phalen invertido negativo. Finkelstein negativo. Tínel negativo. Durkan negativo. Força muscular preservada.</w:t>
      </w:r>
    </w:p><w:p w14:paraId="2296BB13" w14:textId="77777777" w:rsidR="00B018C5" w:rsidRDefault="00B018C5" w:rsidP="00B018C5"><w:pPr><w:jc w:val="both"/><w:rPr><w:b/><w:bCs/></w:rPr></w:pPr></w:p><w:p w14:paraId="5BAEDFB0" w14:textId="6B383B71" w:rsidR="00B018C5" w:rsidRDefault="00B018C5" w:rsidP="00B018C5"><w:pPr><w:jc w:val="both"/></w:pPr><w:r><w:rPr><w:b/><w:bCs/></w:rPr><w:t xml:space="preserve">Exame </w:t></w:r><w:r><w:rPr><w:b/><w:bCs/></w:rPr><w:t>psiquiátrico</w:t></w:r><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Aspecto, postura e higiene: autocuidado preservado (asseado, cabelos alinhados), roupas adequadas para a ocasião e clima, cuidados higiênicos preservados. Cooperativo, idade aparente compatível com a cronológica. Expressão facial sem alterações.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Atitude: ativo. Humor eutímico.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Nível de consciência: vigil, apresenta abertura ocular espontânea, estado alerta e responsivo.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Atenção: preservado, lúcido, auto e alo-orientado, coerente, normovigil, normotenaz. Colaborativo, informava adequadamente, sem distraibilidade, focava bem as respostas.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Orientação no tempo e espaço: Orientado no tempo e no espaço e em relação a si, atenção preservada, com bom contato verbal com discurso lógico-formal sem alterações de curso e/ou conteúdo.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Memória: imediata e remota preservadas.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Inteligência: clinicamente na média populacional para o grupo sócio-cultural e para a faixa etária.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Comportamento: adequado.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Sensopercepção: Não foram observados sinais sugestivos alucinações e/ou ilusões. Com escolha de vocabulário ou escolha das palavras corretas, velocidade normal.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Pensamento: coerente, lógico, com fluxo preservado e conteúdo normal.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Linguagem: velocidade adequada para sua escolaridade, sem neologismos, em tom de volume normal, com boa gramática e sintaxe.</w:t>
      </w:r>
    </w:p>
    <w:p>
      <w:pPr>
        <w:numPr>
          <w:ilvl w:val="0"/>
          <w:numId w:val="1"/>
        </w:numPr>
        <w:jc w:val="both"/>
        <w:spacing w:lineRule="auto"/>
      </w:pPr>
      <w:r>
        <w:rPr/>
        <w:t xml:space="preserve">Juízo crítico: preservado. Capacidade de julgamento, interpretação e raciocínio de forma lógica e corrente.</w:t>
      </w:r>
    </w:p><w:p w14:paraId="11F1C61A" w14:textId="77777777" w:rsidR="00F91E24" w:rsidRDefault="00F91E24" w:rsidP="00B018C5"><w:pPr><w:jc w:val="both"/></w:pPr></w:p><w:p w14:paraId="753CCF12" w14:textId="12669328" w:rsidR="00F91E24" w:rsidRDefault="00F91E24" w:rsidP="00F91E24"><w:pPr><w:jc w:val="both"/></w:pPr><w:r><w:rPr><w:b/><w:bCs/></w:rPr><w:t>Caracterização do Pleito</w:t></w:r><w:r w:rsidRPr="00A50F2E"><w:rPr><w:b/><w:bCs/></w:rPr><w:t>:</w:t></w:r></w:p>
    <w:p>
      <w:pPr>
